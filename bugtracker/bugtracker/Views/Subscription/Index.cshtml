@model IEnumerable<bugtracker.Models.Subscription>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Username
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubscriptionID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubsTypeID);
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SubscriptionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SubscriptionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SubscriptionID })
        </td>
    </tr>
}


</table>

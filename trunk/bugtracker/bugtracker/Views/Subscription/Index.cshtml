@model IEnumerable<bugtracker.Models.LogEvent>

@{
    ViewBag.Title = "Index";
}

<h2>Changes of interest</h2>

<tr>
<td>User who changed</td>
<td>When changed</td>
<td>Comment / What changed</td>
<td>Bug ID#</td>
</tr>


@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            <a href="#" onclick="loadPartialView('Subscription/Details?Sid=@item.SubscriptionID', 'detailview')">Details</a>
        </td>
</tr>
}


</table>
